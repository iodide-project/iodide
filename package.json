{
  "name": "iodide",
  "version": "0.0.3",
  "description": "A notebook-style interface for literate programming in javascript.",
  "main": "index.jsx",
  "author": "Iodide Project Team",
  "license": "MPL-2.0",
  "browserslist": "> 0.25%, not dead",
  "engines": {
    "node": "8.x"
  },
  "scripts": {
    "test": "jest --no-cache",
    "start-and-serve": "webpack-dev-server --watch --mode=development --env=dev",
    "start": "webpack --watch --mode=development --env=dev",
    "build": "webpack",
    "build-production": "webpack --env=production",
    "lint": "eslint src/ test/ --ext=.js --ext=.jsx -f unix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!test-component).+\\.js$"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/styleMock.js"
    },
    "globals": {
      "window": true,
      "IODIDE_BUILD_MODE": "test"
    },
    "setupFiles": [
      "jest-localstorage-mock",
      "<rootDir>/test/globals-test.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ]
  },
  "dependencies": {
    "@iktakahiro/markdown-it-katex": "3.1.0",
