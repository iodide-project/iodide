{
  "name": "iodide",
  "version": "0.0.3",
  "description": "A notebook-style interface for literate programming in javascript.",
  "main": "index.jsx",
  "author": "Iodide Project Team",
  "license": "MPL-2.0",
  "browserslist": "> 0.25%, not dead",
  "engines": {
    "node": "8.x"
  },
  "scripts": {
    "test": "jest --no-cache",
    "test-coverage": "jest --no-cache --coverage",
    "simple-serve": "webpack-dev-server --watch --mode=development --env.NODE_ENV=dev",
    "start": "webpack --watch --mode=development --env.NODE_ENV=dev",
    "build": "webpack --env.NODE_ENV=dev",
    "build-production": "webpack -p",
    "lint": "eslint src/ test/ --ext=.js --ext=.jsx -f unix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!test-component).+\\.js$"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/styleMock.js",
      "monaco-editor/esm/vs/editor/editor.api": "<rootDir>/test/monaco-mock.js"
    },
    "globals": {
      "window": true,
      "USE_LOCAL_PYODIDE": false
    },
    "setupFiles": [
      "jest-localstorage-mock",
      "<rootDir>/test/globals-test.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.20",
    "@emotion/styled": "10.0.17",
    "@iktakahiro/markdown-it-katex": "3.1.0",
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "ajv": "6.10.2",
    "css-element-queries": "1.2.1",
    "date-fns": "2.4.1",
    "debounce-action": "0.1.0",
    "dexie": "2.0.4",
    "dompurify": "2.0.3",
    "emotion": "10.0.17",
    "error-stack-parser": "2.0.4",
    "eslint-config-prettier": "6.3.0",
    "font-awesome": "4.7.0",
    "golden-layout": "1.5.9",
    "jss": "10.0.0",
    "lodash": "4.17.15",
    "lz-string": "1.4.4",
    "markdown-it": "10.0.0",
    "markdown-it-anchor": "5.2.4",
    "markdown-it-emoji": "1.4.0",
    "monaco-editor": "0.18.1",
    "mousetrap": "1.6.3",
    "numeral": "2.0.6",
    "opensans-npm-webfont": "1.0.0",
    "prop-types": "15.7.2",
    "query-string": "6.8.3",
    "react": "16.10.1",
    "react-diff-viewer": "2.0.1",
    "react-dom": "16.10.1",
    "react-helmet-async": "1.0.3",
    "react-jss": "8.6.1",
    "react-popper": "1.3.4",
    "react-redux": "7.1.1",
    "react-table": "6.10.3",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.1",
    "redux-thunk": "2.3.0",
    "stackframe": "1.1.0",
    "throttle-action": "0.1.0",
    "universal-router": "8.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-transform-modules-commonjs": "7.6.0",
    "@babel/preset-env": "7.6.2",
