%% js
var GeoLocation = class GeoLocation {
  constructor(lat, lon) {
    this.lat = lat
    this.lon = lon
  }

  iodideRender() {
    return '<img src="http://staticmap.openstreetmap.de/staticmap.php?center=' + this.lat + ',' + this.lon + '&zoom=17&size=600x300&maptype=mapnik"/>'
  }
}

